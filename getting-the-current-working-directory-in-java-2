        <div id="comments-21274808" class="comments js-comments-container bt bc-black-2 mt12 " data-post-id="21274808" data-min-length="15">
            <ul class="comments-list js-comments-list"
                    data-remaining-comments-count="0"
                    data-canpost="false"
                    data-cansee="true"
                    data-comments-unavailable="false"
                    data-addlink-disabled="true">


    <li id="comment-79971552" class="comment js-comment " data-comment-id="79971552">
        <div class="js-comment-actions comment-actions">
            <div class="comment-score js-comment-edit-hide">
                    <span title="number of &#39;useful comment&#39; votes received"
                            class="cool">2</span>
            </div>
                    </div>
        <div class="comment-text js-comment-text-and-form">
            <div class="comment-body js-comment-edit-hide">
                
                <span class="comment-copy">This returns the location of the JAR file. Not what was asked for.</span>
                
                    &ndash;&nbsp;<a href="/users/207421/user207421"
                       title="272,508 reputation"
                       class="comment-user">user207421</a>
                <span class="comment-date" dir="ltr"><span title="2017-10-01 02:13:38Z" class="relativetime-clean">Oct 1 '17 at 2:13</span></span>
                                                            </div>
        </div>
    </li>
            </ul>
	    </div>

